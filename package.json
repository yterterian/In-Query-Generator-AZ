{
    "name": "AZDataStudioExtension",
    "displayName": "In-Query Generator",
    "publisher": "YT_Dev_Team",
    "description": "An extension to copy query results as an IN SQL statement by Yakov T",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/yterterian/AZDataStudioExtension.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/yterterian/AZDataStudioExtension/issues"
    },
    "homepage": "https://github.com/yterterian/AZDataStudioExtension#readme",
    "activationEvents": [
        "onCommand:extension.copyAsInStatement"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.copyAsInStatement",
                "title": "Copy as IN Statement"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.copyAsInStatement",
                    "when": "editorHasSelection",
                    "group": "9_cutcopypaste"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "node ./node_modules/typescript/bin/tsc -p ./",
        "watch": "node ./node_modules/typescript/bin/tsc -w -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^14.14.6",
        "@types/vscode": "^1.50.0",
        "typescript": "^4.0.3",
        "vscode": "^1.1.34"
    },
    "dependencies": {
        "azdata": "latest",
        "minimatch": "^10.0.1"
    }
}